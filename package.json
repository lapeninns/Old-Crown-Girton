{
  "name": "ship-fast-code",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "postbuild": "next-sitemap",
    "check:links": "node scripts/check-links.mjs http://localhost:3001",
    "build:clean": "npm run clean && npm run build",
    "clean": "rm -rf .next",
    "start": "next start",
    "lint": "next lint",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --config=jest.config.mjs",
    "test:watch": "npm run test -- --watch",
    "test:ci": "NODE_OPTIONS=--experimental-vm-modules jest --config=jest.config.mjs --runInBand --coverage --watchAll=false",
    "test:server": "NODE_OPTIONS=--experimental-vm-modules jest --config=jest.config.mjs --selectProjects server",
    "test:client": "NODE_OPTIONS=--experimental-vm-modules jest --config=jest.config.mjs --selectProjects client",
    "test:api": "npm run test:server -- --testPathPattern=api",
    "test:update-snapshots": "jest -u",
    "test:content": "jest --testPathPattern=content",
    "test:accessibility": "jest --testPathPattern=accessibility",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:install": "npx playwright install --with-deps",
    "validate-content": "node scripts/validate-content.js validate",
    "audit-content": "node scripts/validate-content.js audit",
    "format-content": "node scripts/validate-content.js format",
    "content:validate": "node scripts/validate-content.js validate",
    "content:audit": "node scripts/validate-content.js audit",
    "content:format": "node scripts/validate-content.js format",
    "performance:monitor": "node scripts/performance-monitor.js",
    "cache:stats": "node -e \"const { getCacheStats } = require('./src/lib/data/loader'); console.log(JSON.stringify(getCacheStats(), null, 2));\"",
    "cache:optimize": "node -e \"const { optimizeCache } = require('./src/lib/data/loader'); console.log(JSON.stringify(optimizeCache(), null, 2));\"",
    "cache:clear": "node -e \"const { invalidateCache } = require('./src/lib/data/loader'); console.log('Cleared', invalidateCache(), 'entries');\"",
    "images:analyze": "node scripts/analyze-image-usage.js",
    "images:track": "node scripts/track-image-usage.js",
    "images:cleanup": "node scripts/cleanup-unused-images.js",
    "images:cleanup:move": "node scripts/cleanup-unused-images.js --move",
    "images:structure": "node scripts/create-component-image-structure.js",
    "icons:generate": "node scripts/generate-app-icons.js",
    "images:optimize": "node scripts/optimize-images.js",
    "images:responsive": "node scripts/generate-responsive.js src/assets/images",
    "images:sprite": "node scripts/generate-sprite.js src/assets/images/shared/icons src/assets/images/generated/sprites/icons.svg",
    "lint:image-names": "node scripts/lint-image-names.js",
    "check:image-budgets": "node scripts/check-image-budgets.js --hero-max-kb=200 --page-max-kb=700 --file-max-kb=800"
  },
  "dependencies": {
    "@emotion/is-prop-valid": "^1.3.1",
    "@headlessui/react": "^1.7.18",
    "@supabase/auth-helpers-nextjs": "^0.8.7",
    "@supabase/supabase-js": "^2.41.1",
    "axios": "^1.6.8",
    "caniuse-lite": "^1.0.30001731",
    "client-only": "^0.0.1",
    "crisp-sdk-web": "^1.0.22",
    "eslint": "8.47.0",
    "eslint-config-next": "13.4.19",
    "form-data": "^4.0.0",
    "framer-motion": "^12.23.11",
    "lru-cache": "^11.1.0",
    "lucide-react": "^0.526.0",
    "mailgun.js": "^9.4.1",
    "next": "^14.2.30",
    "next-auth": "^4.24.11",
    "next-sitemap": "^4.2.3",
    "nextjs-toploader": "^1.6.11",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-tooltip": "^5.26.3",
    "server-only": "^0.0.1",
    "stripe": "^13.11.0",
    "swr": "^2.3.6",
    "web-vitals": "^5.1.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@playwright/test": "^1.54.2",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.2",
    "@types/react": "^18.2.73",
    "autoprefixer": "^10.4.19",
    "daisyui": "^4.10.1",
    "jest": "^29.7.0",
    "jest-axe": "^8.0.0",
    "jest-environment-jsdom": "^30.0.5",
    "msw": "^2.10.4",
    "node-mocks-http": "^1.17.2",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.3",
    "undici": "^7.15.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "glob": "^10.4.5",
    "svgo": "^3.3.2"
  },
  "optionalDependencies": {
    "sharp": "^0.34.3"
  }
}
